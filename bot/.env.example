# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=astrology
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_CHAT_HISTORY_LIMIT=5

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_QUEUE=astrology_requests
RABBITMQ_WORKERS=1

# Ollama
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=gpt-oss:latest
ENABLE_THINKING=true
THINKING_MAX_TOKENS=500
THINKING_TEMPERATURE=0.75

# External Services
MEM0_SERVICE_URL=http://localhost:8085
ASTROLOGY_API_URL=http://localhost:8087

# User Management
MAX_STRIKES=3
ENABLE_PROFANITY_FILTER=true

# Chat Encryption (Generate using: python scripts/generate_encryption_key.py)
CHAT_ENCRYPTION_KEY=your_generated_key_here

# App
LOG_LEVEL=INFO